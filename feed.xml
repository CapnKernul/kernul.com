---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Kern.io</title>
  <link href="http://kern.io/"/>
  <link type="application/atom+xml" rel="self" href="http://kern.io/feed.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://kern.io/</id>
  <author>
    <name>Alex Kern</name>
    <email>alex@kern.io</email>
  </author>
  
  {% for post in site.posts %}
    <entry>
      <id>http://kern.io{{ post.id }}</id>
      <link type="text/html" rel="alternate" href="http://kern.io{{ post.url }}"/>
      <title>{{ post.title | xml_escape }}</title>
      <updated>{{ post.date | date_to_xmlschema }}</updated>
      <author>
        <name>Alex Kern</name>
        <uri>http://kern.io/</uri>
      </author>
      <content type="html">{{ post.content | xml_escape }}</content>
    </entry>
  {% endfor %}
</feed>
